<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
                version="2.0">
    <namespace>http://mymam.net/facelets</namespace>
    <tag>
        <tag-name>upload</tag-name>
        <component>
            <component-type>net.mymam.ui.Upload</component-type>
            <renderer-type>net.mymam.ui.UploadRenderer</renderer-type>
        </component>
        <attribute>
            <name>successMsgTemplate</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>warnMsgTemplate</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>errorMsgTemplate</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>addFilesButtonLabel</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>clearButtonLabel</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>submitButtonLabel</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>removeButtonLabel</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>globalStatusTemplate</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>uploadingLabel</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>doneLabel</name>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>failedLabel</name>
            <type>java.lang.String</type>
        </attribute>
    </tag>
</facelet-taglib>
